{
  "name": "formik-material",
  "version": "0.0.1",
  "description": "Material-UI Binds for formik fields",
  "repository": "https://github.com/carlos-algms/formik-material.git",
  "author": "Carlos Gomes <carlos.algms@gmail.com>",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "cross-env NODE_ENV=production build-storybook --output-dir build/storybook",
    "build": "rimraf dist && cross-env NODE_ENV=production tsc",
    "postbuild": "node ./scripts/clear-package-json.js",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test-with-coverage": "yarn test --collect-coverage",
    "type-check": "cross-env NODE_ENV=production tsc --noEmit"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "nanoid": "^3.1.25",
    "react-number-format": "^4.7.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.12.3",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@material-ui/core": "^4.12.3",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.1",
    "@types/lodash.get": "^4.4.6",
    "@types/yup": "^0.29.13",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "jest": "^27.2.0",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "yup": "^0.32.9"
  }
}

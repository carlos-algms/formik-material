{
  "name": "formik-material",
  "version": "3.0.0",
  "description": "Material-UI Binds for formik fields",
  "repository": "https://github.com/carlos-algms/formik-material.git",
  "author": "Carlos Gomes <carlos.algms@gmail.com>",
  "license": "MIT",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "cross-env NODE_ENV=production storybook build --output-dir build/storybook",
    "build": "rimraf dist && cross-env NODE_ENV=production tsc -p ./tsconfig.prod.json && cross-env NODE_ENV=production tsc -p ./tsconfig.prod.json --module commonjs --outDir dist/node",
    "postbuild": "node ./scripts/clear-package-json.js && cp LICENSE README.md ./dist/",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test-with-coverage": "yarn test --collect-coverage",
    "type-check": "cross-env NODE_ENV=production tsc --noEmit",
    "postversion": "node ./scripts/clear-package-json.js"
  },
  "main": "./node/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "nanoid": "^3.3.6",
    "react-number-format": "5.1.4"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.0",
    "formik": "^2.2.9",
    "react": "^17.0.2 || ^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.0",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-docs": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addon-mdx-gfm": "^7.0.5",
    "@storybook/addons": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/react-webpack5": "^7.0.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/lodash.get": "^4.4.7",
    "@types/react": "^18.0.35",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "storybook": "^7.0.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "yup": "^1.1.1"
  },
  "keywords": [
    "formik",
    "material-ui",
    "mui"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">= 14"
  },
  "publishConfig": {
    "access": "public"
  }
}
